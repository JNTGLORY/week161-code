pipeline{
    agent {
        label 'ansible'
    }
    stages{
        stage('check current directory'){
            steps{
                sh 'pwd'
                sh 'whoami'
                sh "mkdir \$(date '+%y%m%d_%H%M%s')"
                sh 'python3 -version'
                sh 'npm --version'


            }
        }
    }
}
